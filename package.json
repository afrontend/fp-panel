{
  "name": "fp-panel",
  "version": "0.0.6",
  "description": "library for fp-block",
  "homepage": "https://github.com/afrontend",
  "author": {
    "name": "Bob Hwang",
    "email": "autoscripts@gmail.com",
    "url": "https://agvim.wordpress.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "panel"
  ],
  "devDependencies": {
    "boxy-jsdoc-template": "^2.0.2",
    "coveralls": "^3.0.2",
    "doxdox": "^3.0.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest": "^23.5.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "test:c": "jest --coverage",
    "docs": "jsdoc --configure .jsdoc.json --verbose",
    "markdown": "doxdox 'lib/**/*.js' --layout markdown --output DOCUMENTATION.md"
  },
  "repository": "https://github.com/afrontend/fp-panel.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "braces": "^3.0.2",
    "handlebars": "^4.7.3",
    "lodash": "^4.17.14",
    "lodash.merge": "^4.6.2",
    "mem": "^5.1.1",
    "tui-jsdoc-template": "^1.2.2"
  }
}
